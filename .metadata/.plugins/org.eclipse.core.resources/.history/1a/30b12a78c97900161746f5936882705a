package footbag;

public class DotUtil {

	public DotUtil(){
		
	}
	
	public byte toByte(String str) {
		byte temp = (byte)0;
		
		return temp;
	}
	
	public String bToString(byte b){
		String temp = "NULL";
		switch(b){
		
		case 0: temp = "air";
		break;
		
		case 1: temp = "sand";
		break;
		
		}
		
		return temp;
	}
	
}
